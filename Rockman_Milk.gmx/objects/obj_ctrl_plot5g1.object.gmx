                {dialogue=true obj_dialogue.realtime=false}
                global.speaker=2;
                global.text="为什么要在自己周围画个洞..."}
    if page=53 {global.speaker=1;
                global.text="我们还是赶紧跳下去救她吧。"}

    if page=54 {dialogue=false
                obj_dialogue.realtime=1
                obj_character.sprite_index=spr_milk_jump
                obj_character.hspeed=2
                obj_character.vspeed=-10
                obj_character.gravity=0.5}
   
                }
else if instance_exists(obj_npc_virgo)
